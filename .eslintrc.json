{
  "env": {
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "airbnb/hooks", "prettier", "prettier/react"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "react-native"],
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      {
        "extension": [".js", ".jsx"]
      }
    ],
    "import/prefer-default-export": "off",
    "react/prop-types": "off"
  }
}
